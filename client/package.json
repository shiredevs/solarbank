{
  "name": "client",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "axios": "^1.7.4"
  },
  "overrides": {
    "micromatch": "4.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "msw": "^2.4.2",
    "postcss": "^8.4.39",
    "prettier": "^3.2.5",
    "serve": "^14.2.3",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "typescript-eslint": "^7.10.0",
    "vite": "^5.3.3",
    "undici": "^6.19.8"
  },
  "scripts": {
    "start:local": "env-cmd -f environment/.env.local vite",
    "build": "vite build",
    "build:serve": "env-cmd -f environment/.env.local vite build && serve -s build",
    "test": "jest --watchAll=false",
    "audit": "npm audit",
    "dependencies": "npm ls --all",
    "lint": "eslint .",
    "format": "prettier --ignore-path ../.gitignore --write \"**/*.+(js|ts|tsx)\""
  },
  "jest": {
    "testEnvironmentOptions": {
      "customExportConditions": [
        ""
      ]
    },
    "preset": "ts-jest",
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "./src/setupTests.ts"
    ],
    "setupFiles": [
      "./src/testSetup/jest.polyfills.ts"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    }
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
